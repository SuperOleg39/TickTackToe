*,
*::before,
*::after
	box-sizing border-box

html
	font-size 20px
	color red

body
	font-size 16px

.board
	width 600px
	margin 0 auto
	border 1px solid red
	&::after
		content ''
		display block
		clear both
	.cell
		float left
		width 100%
		height 200px
		border 1px solid blue
		&.noughts
			background-color gray
		&.toe
			background-color black


@import "pages/admin-page.styl"
